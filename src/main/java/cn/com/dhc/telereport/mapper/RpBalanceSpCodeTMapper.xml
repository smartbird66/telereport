<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cn.com.dhc.telereport.mapper.RpBalanceSpCodeTMapper">
	<!-- select元素表示定义一个查询语句，
			id属性值与接口中的方法名对应，
			resultType属性值指定实体类的全限定名 -->
	<select id="selectAll" resultType="cn.com.dhc.telereport.entity.RpBalanceSpCodeT">
		SELECT balance_sp_code, balance_sp_name FROM rp_balance_sp_code_t
	</select>
</mapper>