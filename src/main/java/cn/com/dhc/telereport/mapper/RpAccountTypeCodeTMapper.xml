<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<!-- namespace属性值与接口全限定名对应 -->
<mapper namespace="cn.com.dhc.telereport.mapper.RpAccountTypeCodeTMapper">
    <!-- select元素表示定义一个查询语句，
            id属性值与接口中的方法名对应，
            resultType属性值指定实体类的全限定名 -->
    <select id="selectAll" resultType="cn.com.dhc.telereport.entity.RpAccountTypeCodeT">
        SELECT account_type_code, account_type_name FROM rp_account_type_code_t</select>
</mapper>
